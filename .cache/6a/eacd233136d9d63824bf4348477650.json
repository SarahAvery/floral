on {\r\n    padding: 0 20px;\r\n  }\r\n}\r\n\r\n@media only screen and (min-width: 630px) {\r\n  .dark-title {\r\n    font-size: 42px;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 360px) {\r\n  .dark-title {\r\n    font-size: 24px;\r\n  }\r\n}\r\n\r\n@media only screen and (min-width: 801px) {\r\n  //  HEADER//\r\n\r\n  .menu-button {\r\n    display: none;\r\n  }\r\n  .icons {\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    width: 100%;\r\n    background-color: rgba(245, 241, 241, 1);\r\n    padding: 5px 20px;\r\n    height: 30px;\r\n\r\n    svg {\r\n      height: 20px;\r\n      width: 20px;\r\n      margin: 0 10px;\r\n      fill: rgba(226, 148, 178, 0.849);\r\n    }\r\n  }\r\n\r\n  .nav-desktop {\r\n    display: block;\r\n    max-width: 60%;\r\n    nav {\r\n      display: block;\r\n      font-size: 18px;\r\n      color: #f7f3f0;\r\n\r\n      ul {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: center;\r\n\r\n        a {\r\n          text-decoration: none;\r\n          list-style: none;\r\n          padding: 0 5px;\r\n          margin: 0 2px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n//TABLET//\r\n\r\n// @media only screen and (max-width: 800px) {\r\n//   //HEADER//\r\n\r\n//     .menu,\r\n//     #menu {\r\n//     position: absolute;\r\n//     top: 0;\r\n//     left: 0;\r\n//     display: flex;\r\n//     flex-wrap: wrap;\r\n//     width: 100%;\r\n//     background-color: #3d3c3c;\r\n//     z-index: 10;\r\n//     margin: 0 auto;\r\n//     justify-content: center;\r\n\r\n//     .header-info {\r\n//       padding: 0;\r\n//     }\r\n\r\n//     .nav-mobile {\r\n//       display: block;\r\n//       width: 100%;\r\n//       text-align: center;\r\n//       font-size: 18px;\r\n\r\n//       a {\r\n//         text-decoration: none;\r\n//         list-style: none;\r\n//       }\r\n//     }\r\n\r\n//     .icons-mobile {\r\n//       display: flex;\r\n//       justify-content: center;\r\n//       width: 100%;\r\n/