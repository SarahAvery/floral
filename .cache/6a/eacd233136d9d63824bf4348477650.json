226, 148, 178, 0.849);\r\n\r\n    .header-info {\r\n      display: flex;\r\n      flex-grow: 1;\r\n      align-items: center;\r\n      justify-content: space-between;\r\n      width: 100%;\r\n      padding: 20px 0;\r\n\r\n      .name {\r\n        font-size: 26px;\r\n        font-weight: 900;\r\n      }\r\n\r\n      .slogan {\r\n        font-size: 18px;\r\n      }\r\n    }\r\n\r\n    .banner {\r\n      width: 100%;\r\n      flex-wrap: wrap;\r\n      text-align: center;\r\n      padding: 60px 0;\r\n\r\n      .title {\r\n        width: 100%;\r\n        font-size: 30px;\r\n        font-weight: 900;\r\n      }\r\n\r\n      .info {\r\n        width: 100%;\r\n        font-size: 18px;\r\n      }\r\n    }\r\n\r\n    .logo {\r\n      justify-content: flex-start;\r\n    }\r\n\r\n    .menu {\r\n      display: none;\r\n    }\r\n  }\r\n\r\n  .menu-button {\r\n    background: none;\r\n    border: none;\r\n    svg {\r\n      height: 30px;\r\n      width: 30px;\r\n    }\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 1450px) {\r\n  //  HEADER//\r\n  .main-header {\r\n    padding: 0 20px;\r\n  }\r\n}\r\n\r\n@media only screen and (min-width: 801px) {\r\n  //  HEADER//\r\n\r\n  .menu-button {\r\n    display: none;\r\n  }\r\n  .icons {\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    width: 100%;\r\n    background-color: rgba(245, 241, 241, 1);\r\n    padding: 0 20px;\r\n    height: 30px;\r\n\r\n    svg {\r\n      height: 20px;\r\n      width: 20px;\r\n      margin: 0 10px;\r\n      fill: rgba(226, 148, 178, 0.849);\r\n    }\r\n  }\r\n\r\n  .nav-desktop {\r\n    display: block;\r\n    max-width: 60%;\r\n    nav {\r\n      display: block;\r\n      font-size: 18px;\r\n      color: #f7f3f0;\r\n\r\n      ul {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: center;\r\n\r\n        a {\r\n          text-decoration: none;\r\n          list-style: none;\r\n          padding: 0 5px;\r\n          margin: 0 2px;\r\n        }\r\n      }\r\n    }